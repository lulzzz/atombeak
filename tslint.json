{
  "rules": {
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "trailing-comma": [true, {"multiline": "never", "singleline": "never"}],
    "semicolon": [true, "never"],
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "arrow-parens": false,
    "max-classes-per-file": false,
    "max-line-length": [true, 180]
  }
}